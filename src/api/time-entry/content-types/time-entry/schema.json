    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "time_entries"
    },
    "task": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::task.task",
      "inversedBy": "time_entries"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}

