{
  "kind": "collectionType",
  "collectionName": "time_entries",
  "info": {
    "singularName": "time-entry",
    "pluralName": "time-entries",
    "displayName": "TimeEntry",
    "description": "Entr√©es de temps"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
